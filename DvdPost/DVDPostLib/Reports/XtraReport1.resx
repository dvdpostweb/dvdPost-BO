  <data name="xrLabel12.Scripts.OnBeforePrint" xml:space="preserve">
    <value>Private Sub OnBeforePrint(ByVal sender As Object, ByVal e As System.Drawing.Printing.PrintEventArgs)
   'Get the current value of the "Fax" data column
   Dim _StrCom as string = GetCurrentColumnValue("communication").ToString()
   Dim _Char as char
   Dim _i as integer
   'Check if the value is null, then cancel the printing
   xrLabel12.text = "+  +  +"
   for _i = 0 to _StrCom.Length - 1
      _char = _strCom.chars(_i)
      xrLabel12.text = xrLabel12.text &amp; "   " &amp; _char
      if _i = 2 then
        xrLabel12.text = xrLabel12.text &amp; "  /"
      end if
      if _i = 6 then
        xrLabel12.text = xrLabel12.text &amp; "  /"
      end if
   Next
   xrLabel12.text = xrLabel12.text &amp; "  +  +  +"

End Sub</value>
